# MultiChain Docker Environment Variables
# Example environment file for multichain-docker
# Copy this to .env and edit values before running `./scripts/setup.sh` or `docker compose`.

CHAIN_NAME=procuchain
RPC_USER=multichainrpc
RPC_PASSWORD=CHANGEME_REPLACE_WITH_SECURE_PASSWORD
MASTER_PORT=7447
RPC_PORT=8000
RPC_HOST=masternode
# WARNING: allowing all RPC access is insecure. Use a restricted CIDR in production.
RPC_ALLOWIP=0.0.0.0/0
# Optional: explorer port override
EXPLORER_PORT=2750
